{
  "name": "portfolios-stats",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "preinstall": "npm run preinstall:rxdb && npm run preinstall:versions",
    "preinstall:rxdb": "(npx yarn@1.13.0 pack --filename rxdb-local.tgz)",
    "preinstall:versions": "node ./scripts/ensure-equal-versions.js",
    "postinstall": "ngcc",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "pouchdb-server --host 0.0.0.0 -p 10101 -m -d /tmp/pouchdb-server/ -n true",
    "server:show": "node server.js",
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build --prod --aot --stats-json && npm run build:copy && echo \"BUILD SUCCESS\"",
    "build:serve": "ws -p 8888 -d dist/angular/",
    "build:serve9000": "ws -p 9000 -d dist/angular/",
    "build:copy": "cp test/multitab.html dist/angular/multitab.html",
    "test": "testcafe chromium test/ --selector-timeout 30000",
    "test:build": "concurrently \"npm run build:serve\" \"npm run test\" --kill-others --success first",
    "lint": "tslint app/src/**/*.ts",
    "analyze": "webpack-bundle-analyzer dist/angular/stats-es2015.json",
    "e2e": "ng e2e",
    "start:electron": "ng build --base-href ./ && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/pwa": "0.1100.7",
    "@angular/router": "~11.1.0",
    "@angular/service-worker": "~11.1.0",
    "@material/card": "^9.0.0",
    "@material/select": "^10.0.0",
    "@material/tabs": "^2.3.0",
    "@stoqey/finnhub": "^0.2.4",
    "bootstrap-vertical-tabs": "^1.2.2",
    "finnhub": "^1.2.1",
    "font-awesome": "4.7.0",
    "moment": "^2.29.1",
    "ngx-moment": "^5.0.0",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-http": "^7.2.2",
    "pouchdb-adapter-idb": "^7.2.2",
    "roboto-npm-webfont": "1.0.1",
    "rxdb": "^9.12.1",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/language-service": "~11.1.1y",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/pouchdb-adapter-http": "^6.1.3",
    "@types/pouchdb-adapter-idb": "^6.1.3",
    "async-test-util": "1.7.3",
    "codelyzer": "^6.0.0",
    "concurrently": "5.3.0",
    "electron": "^11.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "local-web-server": "4.2.1",
    "pouchdb-server": "4.2.0",
    "protractor": "~7.0.0",
    "testcafe": "1.10.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2",
    "webpack-bundle-analyzer": "4.4.0"
  }
}
